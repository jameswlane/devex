{
  "name": "@devex/package-manager-eopkg",
  "private": true,
  "version": "1.8.2",
  "description": "Eopkg plugin for DevEx CLI",
  "main": "main.go",
  "scripts": {
    "build": "go build -o devex-plugin-package-manager-eopkg .",
    "dev": "go build -race -o devex-plugin-package-manager-eopkg main.go",
    "test": "go test ./...",
    "lint": "golangci-lint run",
    "download": "go mod download",
    "mod": "go mod tidy"
  },
  "keywords": [
    "eopkg",
    "solus",
    "linux"
  ],
  "author": "DevEx Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswlane/devex",
    "directory": "packages/package-manager-eopkg"
  },
  "devex": {
    "plugin": {
      "type": "package-manager",
      "platforms": [
        "linux"
      ],
      "dependencies": [
        "eopkg"
      ],
      "priority": 10,
      "supports": {
        "install": true,
        "remove": true,
        "update": true,
        "search": true,
        "list": true
      }
    }
  }
}
