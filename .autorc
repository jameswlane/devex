{
  "plugins": [
    "conventional-commits",
    "git-tag"
  ],
  "owner": "jameswlane",
  "repo": "devex",
  "name": "DevEx Release Bot",
  "email": "action@github.com",
  "noVersionPrefix": false,
  "baseBranch": "main",
  "prereleaseBranches": ["beta", "alpha"],
  "labels": {
    "patch": {
      "name": "patch",
      "changelogTitle": "ğŸ› Bug Fix",
      "description": "A bug fix",
      "releaseType": "patch"
    },
    "minor": {
      "name": "minor",
      "changelogTitle": "ğŸš€ Enhancement",
      "description": "A new feature",
      "releaseType": "minor"
    },
    "major": {
      "name": "major",
      "changelogTitle": "ğŸ’¥ Breaking Change",
      "description": "A breaking change",
      "releaseType": "major"
    },
    "documentation": {
      "name": "documentation",
      "changelogTitle": "ğŸ“ Documentation",
      "description": "Documentation changes",
      "releaseType": "none"
    },
    "internal": {
      "name": "internal",
      "changelogTitle": "ğŸ  Internal",
      "description": "Internal changes",
      "releaseType": "none"
    },
    "ci": {
      "name": "ci",
      "changelogTitle": "ğŸ”§ CI",
      "description": "CI/CD improvements",
      "releaseType": "none"
    }
  },
  "onlyPublishWithReleaseLabel": false,
  "shipit": {
    "onlyGraduateWithReleaseLabel": false,
    "message": "chore: release %v [skip ci]"
  },
  "conventionalCommits": {
    "releaseTypes": {
      "feat": "minor",
      "fix": "patch",
      "perf": "patch",
      "revert": "patch",
      "docs": "none",
      "style": "none",
      "refactor": "patch",
      "test": "none",
      "build": "none",
      "ci": "none",
      "chore": "none"
    }
  }
}