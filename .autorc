{
  "plugins": [
    ["conventional-commits", {
      "releaseTypes": {
        "feat": "minor",
        "fix": "patch",
        "perf": "patch",
        "revert": "patch",
        "docs": "none",
        "style": "none",
        "refactor": "patch",
        "test": "none",
        "build": "none",
        "ci": "none",
        "chore": "none"
      }
    }]
  ],
  "owner": "jameswlane",
  "repo": "devex",
  "name": "DevEx Release Bot",
  "email": "action@github.com",
  "noVersionPrefix": false,
  "baseBranch": "main",
  "prereleaseBranches": ["beta", "alpha"],
  "labels": [
    {
      "name": "patch",
      "changelogTitle": "🐛 Bug Fix",
      "description": "A bug fix",
      "releaseType": "patch"
    },
    {
      "name": "minor",
      "changelogTitle": "🚀 Enhancement",
      "description": "A new feature",
      "releaseType": "minor"
    },
    {
      "name": "major",
      "changelogTitle": "💥 Breaking Change",
      "description": "A breaking change",
      "releaseType": "major"
    },
    {
      "name": "documentation",
      "changelogTitle": "📝 Documentation",
      "description": "Documentation changes",
      "releaseType": "none"
    },
    {
      "name": "internal",
      "changelogTitle": "🏠 Internal",
      "description": "Internal changes",
      "releaseType": "none"
    },
    {
      "name": "ci",
      "changelogTitle": "🔧 CI",
      "description": "CI/CD improvements",
      "releaseType": "none"
    }
  ],
  "onlyPublishWithReleaseLabel": false,
  "shipit": {
    "onlyGraduateWithReleaseLabel": false,
    "message": "chore: release %v [skip ci]"
  }
}